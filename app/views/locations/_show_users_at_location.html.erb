<%= content_tag :div, :class => "location-users-#{params[:location_id]}", :style => "border: solid black 1px;" do %>
  <% unless @users.empty? %>
      <%= content_tag :div, :class=>'menu-holder' do %>
        <%= content_tag :div, :class=>'menu-inner' do %>
          <%= content_tag :ul, :class=>'head', :id=>'main-nav' do %>
            <%= content_tag :li, "Name", :style=>"width: 250px;"   %>
          <% end %>
        <% end %>
      <% end %>
      <%= content_tag :div, '',:class=>'spacer' %>
      <% @users.each do |user| %>
        <%= content_tag :div, :id => "location-user-#{user.id}" do %>
          <%= user.full_name %> &nbsp; <%= link_to "Get Location Back", {:action => "get_location_back_from_user", :user_id => user.id}, :remote => true, :confirm => "Are you Sure?" %>   <br />
        <% end %>
      <% end %>

  <% else %>
    <%= "No Assigned User(s) for this Location." %>
  <% end %>
  <%= link_to "Cancel", '#', :class=>'space-top-10 sub_locations_cancel' %>
<% end %>