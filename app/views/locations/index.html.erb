    <%= content_tag :ul, :class=>'head', :id=>'main-nav' do %>
      <%= content_tag :li, "Name", :style=>"width: 250px;"   %>
      <%= content_tag :li, "Location Type", :style => "width: 250px;" %>
      <%= content_tag :li, "Capacity", :style => "width: 250px;" %>
    <% end %>
  <% end %>
<% end %>
<%= content_tag :div, '',:class=>'spacer' %>
<%= render :partial=>'list' %>

