<%= content_for :sidebar do %>
  <%= render :partial => "sidebar" %>
<% end %>
<%= content_for :js do %>
  <%= javascript_include_tag 'users/index' %>
  <%= javascript_include_tag 'locations/index' %>
<% end %>
<%= content_for :css do %>
  <%= stylesheet_link_tag 'data_list'%>
<% end %>
<%= content_for :footer do %>
  <%#= render :partial=>"footer" %>
<% end %>

