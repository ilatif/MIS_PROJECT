
<%= content_tag :div, :id=>'listings' do %>
  <% @processor_types.each do |type| %>

    <%= content_tag :ul, :class=>"row", :id=>"centre-#{  }" do %>
      <%= content_tag :li, type.name, :style=>"width:75px;" %>
      <%= content_tag :li, type.description, :style=>"width:140px;" %>
      <%= content_tag :li, :class=>'row-options hide' do %>
        <%= content_tag :ul do %>
          <%= content_tag :li do %>
            <%#= link_to 'Show', ticket_path( ticket ), :remote=>true %>
          <% end %>
          <%= content_tag :li, "|" %>
          <%= content_tag :li do %>
            <%#= link_to "Add Comment",
              new_ticket_comment_path( ticket, :ticket=>true ),
              :remote=>true %>
          <% end %>
        <% end %>
      <% end %>


    <% end %>

  <% end %>
<% end %>
